{
  "name": "olmap_ui",
  "version": "0.2.2",
  "private": true,
  "description": "UI for the Open Logistics Map application",
  "scripts": {
    "start": "react-scripts start --no-cache",
    "build": "DISABLE_ESLINT_PLUGIN=true TSC_COMPILE_ON_ERROR=true react-scripts build",
    "test": "react-scripts test --no-cache",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "eject": "react-scripts eject",
    "format": "biome format --write .",
    "format:check": "biome format ."
  },
  "author": "Johan Lindqvist / Forum Virium Helsinki",
  "dependencies": {
    "@rjsf/bootstrap-4": "^5.24.13",
    "@rjsf/core": "^5.24.13",
    "@rjsf/utils": "^5.24.13",
    "@rjsf/validator-ajv8": "^5.24.13",
    "@sentry/browser": "^10.38.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@turf/turf": "^7.2.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "bootstrap": "^4.6.2",
    "geolib": "^3.3.4",
    "http-proxy-middleware": "^3.0.5",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "mapbox-gl": "^1.13.3",
    "mapbox-gl-leaflet": "^0.0.16",
    "moment": "^2.30.1",
    "osmtogeojson": "^3.0.0-beta.5",
    "react": "^18.3.1",
    "react-bootstrap": "^1.6.8",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^5.3.4",
    "react-scripts": "^5.0.1",
    "reactstrap": "^8.10.1",
    "sass": "^1.94.0",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "@turf/difference": "7.0.0"
  },
  "overrides": {
    "typescript": "^5.9.3"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-leaflet|@react-leaflet|@turf|polyclip-ts|splaytree-ts|geokdbush|tinyqueue)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/node_modules/react-scripts/config/jest/cssTransform.js"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@playwright/test": "^1.56.1",
    "@types/leaflet": "^1.9.15",
    "@types/lodash": "^4.17.14",
    "@types/react-leaflet": "^2.8.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/reactstrap": "^8.7.2"
  }
}
